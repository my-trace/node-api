{
  "name": "mytrace-node-api",
  "version": "1.0.0",
  "description": "web API for mytrace",
  "main": "server/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "start-dev": "nodemon server/index.js",
    "gen-migration": "bin/knex migrate:make",
    "migrate": "bin/knex migrate:latest",
    "fixtures": "node scripts/load-fixtures.js",
    "psql": "psql $PG_URL",
    "test": "ava",
    "lint": "node_modules/.bin/eslint ."
  },
  "ava": {
    "files": "test/specs/*.js",
    "concurrency": 10,
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/my-trace/node-api.git"
  },
  "author": "Andrew Carlson <acarl005@g.ucla.edu>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/my-trace/node-api/issues"
  },
  "homepage": "https://github.com/my-trace/node-api#readme",
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "envalid": "^2.4.1",
    "express": "^4.14.0",
    "grant-koa": "^3.6.5",
    "knex": "^0.12.6",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.3.0",
    "koa-cors": "0.0.16",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-session": "^3.4.0",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "node-cache": "^4.1.0",
    "pg": "^6.1.0",
    "request-promise": "^4.1.1",
    "uuid": "^3.0.1",
    "winston": "^2.3.0",
    "winston-papertrail": "^1.0.4"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "eslint": "^3.11.1",
    "nodemon": "^1.11.0",
    "supertest-koa-agent": "^0.3.0"
  }
}
